<h2>Inventory</h2>
<table>
  <tr>
    <th>Title</th>
    <th>Author</th>
    <th>Publisher</th>
    <th>Genre</th>
    <th>Price (Markup)</th>
    <th>Stock</th>
    <th>Copies Sold</th>
  </tr>
  <% @books.each do |book|%>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.author().print_name %></td>
      <td><%= book.publisher().name %></td>
      <td><%= book.genre %></td>
      <td><%= book.print_sell_price() %> (+Â£markup)</td>
      <td><%= book.stock_count %></td>
      <td><%= book.copies_sold %></td>
    </tr>
  <% end %>
</table>