<div class="subheader">
  <h2>All Publishers</h2>
  <a href="/publishers/new" class="btn">Create New</a>
</div>
<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Phone Number</th>
  </tr>
  <% @publishers.each do |publisher|%>
    <tr>
      <td>
        <a href="/publishers/<%= publisher.id %>"><%= publisher.name %></a>
        <a class="btn amber" href="mailto: <%= publisher.email %>" target="_blank">Email</a>
      </td>
      <td><%= publisher.address %></td>
      <td><%= publisher.phone_number %></td>
    </tr>
  <% end %>
</table>